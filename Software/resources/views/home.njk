        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown"><b>Login</b> <span class="caret"></span></a>
          <ul id="login-dp" class="dropdown-menu">
            <li>
              {{ form.open({action: 'UsersController.login'}) }}
              {{ csrfField }}
              <div class="row">
                <div class="col-md-12">
                  <form class="form" role="form" method="post" action="login" accept-charset="UTF-8" id="login-nav">
                    <div class="form-group">
                      <font color="black">
                        <label class="sr-only" for="exampleInputEmail2">Correo Electrónico</label>
                        {{ form.label('email', 'Correo') }}
                        {{ form.text('email', null, {class:'form-control'}) }}
                      </font>
                    </div>
                    <div class="form-group">
                      <font color="black">
                      {{ form.label('password', 'Password') }}
                      {{ form.password('password', null, {class:'form-control'}) }}
                      </font>
                    </div>
                    <div>
                      {{ form.submit('Entrar', 'entrar', { class: 'btn btn-primary btn-block' }) }}
                    </div>
                  </form>
                </div>
                <div class="bottom text-center">
                  <font color="black">¿Nuevo?</font> <a href="#"><b><font color="black">Registrarse</font></b></a>
                </div>
              </div>
              {{ form.close() }}
            </li>
          </ul>
        </li>
      </ul>
    </div>
